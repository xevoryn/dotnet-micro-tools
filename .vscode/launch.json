{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch GetFormattedDateTime",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build GetFormattedDateTime",
            "program": "${workspaceFolder}/src/GetFormattedDateTime/bin/Debug/net5.0/GetFormattedDateTime.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/GetFormattedDateTime",
            "console": "externalTerminal",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}
